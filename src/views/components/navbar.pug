.navbar.container#navbar
  .contained
    .right
      a.title.link(href="/") 
        img(src="/public/img/logo.svg")
        | {web.lab}
    .left
      a(href="#main-menu"
        role="button"
        aria-expanded="false"
        aria-controls="main-menu"
        aria-label="Open main menu").menu-toggle
        i.fa.fa-bars
      .main-menu#main-menu(role="navigation"
       aria-expanded="false"
       aria-label="Main menu")
        a.link(href="/contest") contest
        a.link(href="/class") class
        a.link(href="/schedule") schedule
        a.link(href="/team") team
        a.link(href="/winners") winners
        a.link(href="/FAQ") FAQ
        a(href="#main-menu-toggle"
        role="button"
        aria-expanded="false"
        aria-controls="main-menu"
        aria-label="Close main menu").menu-close
          i.fa.fa-times
      a(href="#main-menu-toggle"
        tabindex="-1"
        aria-hidden="true"
        hidden).backdrop
      
